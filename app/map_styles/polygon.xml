        <Rule>
            <MaxScaleDenominator>17061</MaxScaleDenominator>
            <MinScaleDenominator>4264</MinScaleDenominator>
            <LineSymbolizer stroke="#888" stroke-width="1.0"/>
        </Rule>
        <Rule>
            <MaxScaleDenominator>4264</MaxScaleDenominator>
