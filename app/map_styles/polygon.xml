        <Rule>
            <Filter>[likes] &gt;= 3 and [likes] &lt; 10</Filter>
            <PolygonSymbolizer fill="#feb24c" />
        </Rule>
        <Rule>
            <Filter>[likes] = 2</Filter>
            <PolygonSymbolizer fill="#fed976" />
        </Rule>
        <Rule>
            <Filter>[likes] = 1</Filter>
            <PolygonSymbolizer fill="#ffe8a9" />
