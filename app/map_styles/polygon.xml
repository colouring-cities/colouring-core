    <Style name="likes">
        <Rule>
            <Filter>[likes] &gt;= 100</Filter>
            <PolygonSymbolizer fill="#bd0026" />
        </Rule>
        <Rule>
            <Filter>[likes] &gt;= 50 and [likes] &lt; 100</Filter>
            <PolygonSymbolizer fill="#e31a1c" />
        </Rule>
        <Rule>
